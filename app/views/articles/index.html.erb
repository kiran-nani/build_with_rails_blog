<h1>Articles</h1>
<%= link_to "Home", root_path %> 
<%= link_to "New Blog", new_article_path %> 
<% @articles.each do |article| %>
<div class="article">
  <h3><%= article.title %></h3>
  <h3><%= article.body %></h3>
  <%= link_to "show", article_path(article) %>
  <%= link_to "Edit", edit_article_path(article) %>
</div>
<% end %>

